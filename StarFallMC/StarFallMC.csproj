        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="assets\DefaultGameIcon\snapshot.png" />
      <Resource Include="assets\DefaultGameIcon\snapshot.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="assets\DefaultGameIcon\LiteLoader.png" />
      <Resource Include="assets\DefaultGameIcon\LiteLoader.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="assets\element\bedrock.png" />
      <Resource Include="assets\element\bedrock.png">
