<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net8.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UseWPF>true</UseWPF>
        <Version>0.0.1</Version>
        <Title>StarFallMC</Title>
        <Authors>StarFall</Authors>
        <Description>A Minecraft launcher</Description>
        <Copyright>StarFall</Copyright>
        <ApplicationIcon>assets\ico.ico</ApplicationIcon>
        <PackageIcon>assets\ico.ico</PackageIcon>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="assets\ico.ico" />
      <Resource Include="assets\ico.ico">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="assets\DefaultGameIcon\Minecraft.png" />
      <Resource Include="assets\DefaultGameIcon\Minecraft.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="assets\DefaultGameIcon\NeoForge.png" />
      <Resource Include="assets\DefaultGameIcon\NeoForge.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="assets\DefaultGameIcon\steve.png" />
      <Resource Include="assets\DefaultGameIcon\steve.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="assets\steve.png" />
      <Resource Include="assets\steve.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="assets\DefaultGameIcon\unknowGame.png" />
