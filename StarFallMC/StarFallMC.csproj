        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="assets\DefaultGameIcon\curseForge.png" />
      <Resource Include="assets\DefaultGameIcon\curseForge.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="assets\DefaultGameIcon\modrinth.png" />
